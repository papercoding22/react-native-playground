{
  "name": "RNStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "test:ci": "yarn run test --coverage",
    "pod-install": "npx pod-install",
    "prettier": "prettier --write src/**/*.ts src/**/*.tsx",
    "clean-node": "rm -rf node_modules/ && yarn cache clean && yarn install",
    "launch-ios-simulator": "xcrun simctl boot 'iPhone 15 Pro'"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.2",
    "@tanstack/react-query": "^5.28.8",
    "axios": "^1.6.8",
    "i18next": "^23.10.1",
    "react": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.6",
    "react-native-error-boundary": "^1.2.4",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.30.1",
    "react-native-svg": "^15.1.0",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@testing-library/react-native": "^12.4.4",
    "@tsconfig/react-native": "^3.0.4",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.70",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.6.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "jest-transformer-svg": "^2.0.2",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18"
  }
}
